           refetchQueries: [{ query: GET_USER_DETAILS, variables: { email: userID } }],\n        })\n        form.resetFields();\n        setVisible(false);\n    }\n    const onFormFail = (value) => {\n        setVisible(true);\n    }\n\n    return (\n        <Form\n            labelCol={{ span: 4 }}\n            wrapperCol={{ span: 14 }}\n            layout=\"horizontal\"\n            form={form}\n            onFinish={onFinish}\n            onFinishFailed={onFormFail}\n            id='new_habit'\n        >\n            <Form.Item\n                name='habit_name'\n                label=\"Habit\"\n                rules={[{ required: true, message: \"Please input Habit's Name!\" }]}\n            >\n                <Input placeholder=\"Enter Name of the Habit\" />\n            </Form.Item>\n            <Form.Item\n                name='type'\n                label=\"Type\"\n                rules={[{ required: true, message: \"Please Select Type of Habit!\" }]}\n            >\n                <Select onChange={(type)=>setHabitType(type)} placeholder=\"Enter Type of Habit\">\n                    <Select.Option value=\"CHECK\">Yes/No</Select.Option>\n                    <Select.Option value=\"REPS\">Integer Habit(Reps)</Select.Option>\n                    <Select.Option value=\"DURATION\">Timed Habit</Select.Option>\n                    {/* <Select.Option value=\"DURATION\">Bad Habit</Select.Option> */}\n                </Select>\n            </Form.Item>\n            <Form.Item\n                name='date_range'\n                label=\"Date\"\n                rules={[{ required: true, message: \"Please Select date!\" }]}\n            >\n                <RangePicker />\n            </Form.Item>\n            { (habitType==='REPS') &&\n                <Form.Item name=\"reps_no\" label=\"Count\">\n                    <InputNumber />\n                </Form.Item>\n            }\n            { (habitType==='DURATION') &&\n                <Form.Item name=\"timer\" label=\"Timer\">\n                    <TimePicker showNow={false} />\n                </Form.Item>\n            }\n        </Form>\n    )\n}\n","/Users/rto/Desktop/Habit-Tracker/frontend/src/components/dashboardComponents/HabitCard.js",["58","59"],{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"64","severity":1,"message":"65","line":9,"column":10,"nodeType":"66","messageId":"67","endLine":9,"endColumn":20},{"ruleId":"64","severity":1,"message":"68","line":25,"column":28,"nodeType":"66","messageId":"67","endLine":25,"endColumn":44},{"ruleId":"64","severity":1,"message":"69","line":8,"column":3,"nodeType":"66","messageId":"67","endLine":8,"endColumn":19},{"ruleId":"64","severity":1,"message":"70","line":10,"column":3,"nodeType":"66","messageId":"67","endLine":10,"endColumn":15},{"ruleId":"64","severity":1,"message":"71","line":22,"column":26,"nodeType":"66","messageId":"67","endLine":22,"endColumn":32},{"ruleId":"64","severity":1,"message":"72","line":4,"column":5,"nodeType":"66","messageId":"67","endLine":4,"endColumn":15},{"ruleId":"64","severity":1,"message":"73","line":11,"column":5,"nodeType":"66","messageId":"67","endLine":11,"endColumn":11},{"ruleId":"64","severity":1,"message":"74","line":17,"column":7,"nodeType":"66","messageId":"67","endLine":17,"endColumn":22},{"ruleId":"64","severity":1,"message":"75","line":44,"column":26,"nodeType":"66","messageId":"67","endLine":44,"endColumn":30},{"ruleId":"64","severity":1,"message":"76","line":44,"column":32,"nodeType":"66","messageId":"67","endLine":44,"endColumn":37},{"ruleId":"64","severity":1,"message":"77","line":6,"column":10,"nodeType":"66","messageId":"67","endLine":6,"endColumn":16},{"ruleId":"64","severity":1,"message":"78","line":6,"column":31,"nodeType":"66","messageId":"67","endLine":6,"endColumn":38},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"no-unused-vars","'useHistory' is defined but never used.","Identifier","unusedVar","'setAuthenticated' is assigned a value but n